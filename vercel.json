{
    "version": 2,

    "builds": [{
        "src": "ari-fitness-api/dist/main.js",
        "use": "@vercel/node"
    }],
    "installCommand": "npm install",
    "public": true,
    "devCommand": "npm run start:dev",
    "routes": [{
        "src": "/(.*)",
        "dest": "ari-fitness-api/dist/main.js",
        "methods": [
            "GET",
            "POST",
            "PUT",
            "PATCH",
            "DELETE",
            "HEAD",
            "OPTIONS"
        ]
    }],
    "buildCommand": "npm run build"
}